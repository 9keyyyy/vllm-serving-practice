{
    "id": null,
    "uid": "llm-serving-metrics",
    "title": "LLM Serving Metrics",
    "tags": ["llm", "vllm", "metrics"],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 0,
    "refresh": "5s",
    "time": {
      "from": "now-15m",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Requests per Second",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "rate(http_requests_total[1m])",
            "legendFormat": "{{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "options": {}
      },
      {
        "id": 2,
        "title": "P95 Latency",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "P95 {{method}} {{endpoint}}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "options": {}
      },
      {
        "id": 3,
        "title": "Batch Size Distribution",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "rate(batch_size_total_sum[5m]) / rate(batch_size_total_count[5m])",
            "legendFormat": "Avg Batch Size",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "options": {}
      },
      {
        "id": 4,
        "title": "vLLM Request Success Rate",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "rate(vllm_requests_total{status=\"success\"}[1m])",
            "legendFormat": "Success",
            "refId": "A"
          },
          {
            "expr": "rate(vllm_requests_total{status=\"error\"}[1m])",
            "legendFormat": "Error",
            "refId": "B"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "options": {}
      },
      {
        "id": 5,
        "title": "vLLM Latency",
        "type": "timeseries",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "rate(vllm_latency_seconds_sum[1m]) / rate(vllm_latency_seconds_count[1m])",
            "legendFormat": "Avg vLLM Latency",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        },
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "custom": {
              "drawStyle": "line"
            }
          }
        },
        "options": {}
      },
      {
        "id": 6,
        "title": "HTTP Status Codes",
        "type": "piechart",
        "datasource": {
          "type": "prometheus",
          "uid": "prometheus"
        },
        "targets": [
          {
            "expr": "sum by (status) (http_requests_total)",
            "legendFormat": "{{status}}",
            "refId": "A",
            "format": "table"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        },
        "options": {
          "legend": {
            "displayMode": "visible",
            "values": ["value"]
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single"
          },
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {}
          }
        }
      }
    ]
}
